{
  "author": "mklabs",
  "name": "gimme-assets",
  "description": "A little, experimental tool to manage web assets from the CLI. Based on cdnjs / microjs libs",
  "keywords": [
    "assets",
    "cli",
    "microjs"
  ],
  "version": "0.2.0",
  "main": "lib/gimme",
  "bin": {
    "gimme": "bin/gimme"
  },
  "engines": {
    "node": "0.6.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mklabs/gimme-assets.git"
  },
  "dependencies": {
    "ronn": "~0.3.8",
    "github-flavored-markdown": "~1.0.1",
    "tabtab": "0.0.2",
    "flatiron": "~0.1.16",
    "nopt": "~1.0.10",
    "director": "~1.0.9-1",
    "cfg": "0.0.2",
    "prompt": "~0.1.12",
    "underscore": "~1.3.1",
    "request": "~2.9.153",
    "tar": "~0.1.13",
    "uglify-js": "~1.2.6",
    "glob": "~3.1.9",
    "fstream": "~0.1.18",
    "minimatch": "~0.2.4"
  },
  "scripts": {
    "postuninstall": "node scripts/uninstall",
    "pretest": "rm -rf test/fixtures/",
    "test": "node test --filter test-api --log"
  }
}
