{
  "author": "mklabs",
  "name": "gimme-assets",
  "description": "A little, experimental tool to manage web assets from the CLI. Based on cdnjs / microjs libs",
  "keywords": ["assets", "cli", "microjs"],
  "version": "0.1.1",
  "main": "lib/gimme",
  "bin": {
    "gimme": "bin/gimme"
  },
  "engines": {
    "node": "0.6.x"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mklabs/gimme-assets.git"
  },
  "dependencies": {
    "ronn": "~0.3.8",
    "github-flavored-markdown": "~1.0.1",
    "tabtab": "0.0.2",
    "flatiron": "~0.1.16",
    "nopt": "~1.0.10",
    "director": "~1.0.9-1",
    "cfg": "0.0.2",
    "prompt": "~0.1.12"
  },
  "scripts": {
    "install": "cd lib/gimme/fetch && npm install --production",
    "postuninstall": "node scripts/uninstall",
    "pretest": "rm -rf test/fixtures/",
    "test": "node test --log"
  }
}
